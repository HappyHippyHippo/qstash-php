{
    "name": "heyjorgedev/qstash-php",
    "description": "This is my package qstash-php",
    "keywords": [
        "heyjorgedev",
        "qstash-php"
    ],
    "homepage": "https://github.com/heyjorgedev/qstash-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Lapa",
            "email": "2780099+heyjorgedev@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.8.1",
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "HeyJorgeDev\\QStash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeyJorgeDev\\QStash\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
